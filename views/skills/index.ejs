<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
   <link rel="stylesheet" href="styles.css">
    <title>Document</title>
</head>
<body>
    <h1>These are my skills</h1>
    <a href="/skills/new">Add Skill</a>
    <ul>
      <% skills.forEach(function(t) { %>
        <li>
            <a href="/skills/<%= t.id %>"><%= t.skill %></a>

            <form method="POST" action="/skills/<%=t.id%>?_method=DELETE">
                <button type="submit">Delete Skill</button>
                </form>
        </li>
      <% }); %>
    </ul>
    
    
</body>
</html>